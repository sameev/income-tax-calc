{
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
},
  "extends": [
    "next",
    "next/core-web-vitals",
    "plugin:@typescript-eslint/recommended",
    "prettier"
  ],
  "rule": {
    "react/react-in-jsx-scope": "off",
    "no-unused-vars":"warn",
    "react/prop-types": "off"
  }
}
